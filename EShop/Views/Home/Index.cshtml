            <script src="~/Scripts/vendors/ui-bootstrap-custom-tpls-1.3.3.min.js"></script>
        <script src="~/Scripts/vendors/ui-bootstrap-custom-1.3.3.js"></script>*@
    @*<script src="Scripts/shop/home/rootCtrl.js"></script>*@
    <script src="~/Scripts/vendors/ngStorage.js"></script>
    <script src="~/Scripts/shop/services/cartService.js"></script>
